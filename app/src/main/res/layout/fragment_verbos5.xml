    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnUñchukiyaña"
        android:layout_width="200dp"
        android:layout_height="51dp"
        android:background="@drawable/botonazul"
        android:text="uñch'ukiyaña "
