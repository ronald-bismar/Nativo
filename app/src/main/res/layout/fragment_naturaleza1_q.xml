        app:layout_constraintBottom_toTopOf="@+id/imageView30"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView77"
        app:layout_constraintTop_toBottomOf="@+id/textView96"
        app:layout_constraintVertical_bias="0.5" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnPanqara"
        android:layout_width="216dp"
        android:layout_height="48dp"
        android:background="@drawable/botonverde"
        android:fontFamily="@font/arista"
        android:text="t'ika"
        android:textAllCaps="false"
        android:textColor="#027167"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnPallqa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnAli"
        app:layout_constraintVertical_bias="0.5" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnAli"
        android:layout_width="216dp"
        android:layout_height="48dp"
        android:background="@drawable/botonverde"
        android:fontFamily="@font/arista"
        android:text="sach'a"
        android:textAllCaps="false"
        android:textColor="#027167"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/btnPanqara"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView30"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/textView96"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/color1"
        android:text="ELIGE LA PALABRA CORECTA"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="25dp"
        android:textStyle="bold"
        android:paddingTop="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>