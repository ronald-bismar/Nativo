<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout57"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ClaseColores.FragmentColores10">

    <TextView
        android:id="@+id/textView82"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/linottebold"
        android:text="Café"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/txtMallku"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView36"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/textView36"
        style="@style/textoGuia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/linottebold"
        android:paddingLeft="20dp"
        android:text="Arrastra la opcion correcta al contenedor"
        app:layout_constraintBottom_toTopOf="@+id/textView82"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/imageView17"
        style="@style/estiloDeTexto"
        android:layout_width="160dp"
        android:layout_height="160dp"
        android:background="@drawable/bordesredondeados"
        android:backgroundTint="#9C2905"
        android:gravity="center"
        android:src="@mipmap/condor"
        android:text="café"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/txtContPalabra2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtMallku"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtMallku"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/linottebold"
        android:gravity="center"
        android:text="chuqña"
        android:textAllCaps="false"
        android:textColor="@color/colorBlack"
        android:textSize="30sp"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView17"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtChumpi"
        app:layout_constraintTop_toBottomOf="@+id/textView82"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtChumpi"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/linottebold"
        android:gravity="center"
        android:text="ch'umpi"

        android:textAllCaps="false"
        android:textColor="@color/colorBlack"
        android:textSize="30sp"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView17"
        app:layout_constraintEnd_toStartOf="@+id/txtMallku"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView82"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtSuri"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/linottebold"
        android:gravity="center"
        android:text="larama"
        android:textAllCaps="false"
        android:textColor="@color/colorBlack"
        android:textSize="30sp"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toTopOf="@+id/btnComprobar9"
        app:layout_constraintEnd_toStartOf="@+id/txtChiñi"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtContPalabra2"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtChiñi"
        android:layout_width="121dp"
        android:layout_height="42dp"
        android:background="@drawable/button_background"
        android:fontFamily="@font/linottebold"
        android:gravity="center"
        android:text="ch'iqi"
        android:textAllCaps="false"
        android:textColor="@color/colorBlack"
        android:textSize="30sp"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toTopOf="@+id/btnComprobar9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtSuri"
        app:layout_constraintTop_toBottomOf="@+id/txtContPalabra2"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/txtContPalabra2"
        android:layout_width="189dp"
        android:layout_height="43dp"
        android:background="@drawable/strokedelgado"
        android:gravity="center"
        style="@style/textoGuia"
        app:layout_constraintBottom_toTopOf="@+id/txtSuri"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView17"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/btnComprobar9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#1597FF"
        android:fontFamily="@font/linottebold"
        android:text="COMPROBAR"
        android:textSize="20dp"
        app:cornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtSuri"
        app:layout_constraintVertical_bias="0.5" />

</androidx.constraintlayout.widget.ConstraintLayout>